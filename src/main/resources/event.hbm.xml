<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="hibernate.EventTable" table="event">
        <id name="id" column="ID" type="java.lang.Integer">
            <generator class="increment" />
        </id>
        <property name="user_id" column="User_ID" type="java.lang.Integer" />
        <property name="place_id" column="Place_ID" type="java.lang.Integer" />
        <property name="event" column="Event" type="java.lang.String" />

    </class>
</hibernate-mapping>